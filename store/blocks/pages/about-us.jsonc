    "children": ["flex-layout.row#about-us__section--two-container-1"],
    "props": {
      "blockClass": "container-about-us"
    }
  },
  "flex-layout.row#about-us__section--two-container-1": {
    "children": [
      "flex-layout.col#about-us__section--two--one",
      "flex-layout.col#about-us__section--two--two"
    ],
    "props": {
      "blockClass": "container-about-us-one"
    }
